{
  "name": "react-whatsapp-styled-ui-hebrew",
  "description": "A Whatsapp chat screenshot design, build with React, TS, CSS and styled-components.",
  "version": "1.0.0",
  "private": false,
  "main": "dist/index.ts",
  "module": "dist/index.ts",
  "files": [
    "dist",
